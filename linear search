#include <stdio.h>
int main ()
{
    int a[] = {200, 500, 600, 10, 90, 7, 120};
    int key;
    printf("enter search key: ");
    scanf("%d", &key);

    
    for(int i=0; i<7; i++)
    {
       
        if(a[i] == key)
        {
            printf("Key found at index: %d", i);
            break;
        }
        else
        {
            printf("not found");
            return 1;
        }
            
    }
    
}
